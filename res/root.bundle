nt": [0, 0, 0, 0], "weather": {"snow": {"velocity": 10, "rate": 10, "enabled": false, "height": 25}}, "id": "0bf61a6a383042aabde55311c9c466ea.environment", "name": "Default Environment"}, "2624615ac9904e969f4059bcaa2ca7f8.texture": {"wrapS": "Repeat", "minFilter": "Trilinear", "imageRef": "75202fa78b2505cd26136ec764148f351bf26ad0.png", "license": "CC0", "flipY": true, "deleted": false, "name": "e9f8b43ec479bb57a356cce44e5459045b324f87.png", "magFilter": "Bilinear", "created": "2015-06-25T08:21:43.760611+00:00", "dataModelVersion": 2, "modified": "2015-06-25T08:21:45.502367+00:00", "repeat": [1, 1], "lodBias": 0, "offset": [0, 0], "id": "2624615ac9904e969f4059bcaa2ca7f8.texture", "generateMipmaps": true, "anisotropy": 1, "wrapT": "Repeat"}, "95ac417c09514b26b3b3740443e4fe26.entity": {"name": "boat", "license": "CC0", "created": "2016-08-04T11:07:04.904Z", "deleted": false, "tags": {}, "dataModelVersion": 2, "modified": "2016-08-04T11:07:04.904Z", "static": false, "components": {"transform": {"rotation": [0, 0, 0], "scale": [0.01, 0.01, 0.01], "translation": [0, 0, 12.74325180053711], "children": {"c591d4f1b5ea436b92cfa972ea4b88f7.entity": {"entityRef": "c591d4f1b5ea436b92cfa972ea4b88f7.entity", "sortValue": 1}}}}, "hidden": false, "id": "95ac417c09514b26b3b3740443e4fe26.entity"}, "70e02155cb084f17a44b75ad1d78fed8.script": {"body": "// Add external resource: https://code.gooengine.com/0.12.6/lib/waterpack.js\r\nvar setup = function(args, ctx, goo) {\r\n\tctx.world.gooRunner.callbacksNextFrame.push(function () \r\n\t{\r\n\t\tif(undefined === goo.FlatWaterRenderer){\r\n\t\t\tconsole.warn('Need to include the waterpack.js as an external resource!');\r\n\t\t\tconsole.warn('https://code.gooengine.com/latest/lib/waterpack.js');\r\n\t\t} else {\r\n\t\t\tvar settings = {\r\n\t\t\t\tdivider: args.divider,\r\n\t\t\t\tupdateWaterPlaneFromEntity: false,\r\n\t\t\t\tuseRefraction: args.useRefraction\r\n\t\t\t};\r\n\t\t\tif (args.tex) {\r\n\t\t\t\tsettings.normalsTexture = args.tex;\r\n\t\t\t}\r\n\t\t\tvar waterRenderer = new goo.FlatWaterRenderer(settings);\r\n\t\t\twaterRenderer.followCam = false;\r\n\t\t\twaterRenderer.waterMaterial.setTexture('NORMAL_MAP', args.tex);\r\n\t\r\n\t\t\twaterRenderer.waterMaterial.shader.uniforms.waterScale = args.waterScale;\r\n\t\t\twaterRenderer.waterMaterial.shader.uniforms.waterColor = args.waterColor;\r\n\t\t\twaterRenderer.waterMaterial.shader.uniforms.timeMultiplier = args.timeMultiplier;\r\n\t\r\n\t\t\twaterRenderer.waterMaterial.shader.uniforms.distortionMultiplier = args.distortionMultiplier;\r\n\t\t\twaterRenderer.waterMaterial.shader.uniforms.fresnelPow = args.fresnelPow;\r\n\t\t\twaterRenderer.waterMaterial.shader.uniforms.normalMultiplier = args.normalMultiplier;\r\n\t\t\twaterRenderer.waterMaterial.shader.uniforms.fresnelMultiplier = args.fresnelMultiplier;\r\n\t\r\n\t\t\twaterRenderer.waterMaterial.shader.uniforms.sunDirection = args.sunDirection;\r\n\t\t\twaterRenderer.waterMaterial.shader.uniforms.sunSpecPower = args.sunSpecPower;\r\n\t\t\twaterRenderer.waterMaterial.shader.uniforms.sunColor = args.sunColor;\r\n\t\t\twaterRenderer.waterMaterial.shader.uniforms.sunShininess = args.sunShininess;\r\n\t\r\n\t\t\twaterRenderer.waterMaterial.shader.uniforms.doFog = args.doFog;\r\n\t\t\twaterRenderer.waterMaterial.shader.uniforms.fogColor = args.fogColor;\r\n\t\t\twaterRenderer.waterMaterial.shader.uniforms.fogStart = args.fogStart;\r\n\t\t\twaterRenderer.waterMaterial.shader.uniforms.fogScale = args.fogScale;\r\n\t\r\n\t\t\tctx.world.gooRunner.renderSystem.preRenderers.push(waterRenderer);\r\n\t\t\r\n\t\t\twaterRenderer.setWaterEntity(ctx.entity);\r\n\t\t\t\r\n\t\t\tvar all = ctx.world.gooRunner.world.getEntities();\r\n\t\t\tfor (var i = 0; i < all.length; i++) {\r\n\t\t\t\tvar ent = all[i];\r\n\t\t\t\tif (ent !== ctx.entity && ent.meshRendererComponent) {\r\n\t\t\t\t\tent.meshRendererComponent.isReflectable = true;\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t\t\r\n\t\t\tvar rEnts = ctx.world.gooRunner.renderSystem._activeEntities;\r\n\t\t\tfor (i = 0; i < rEnts.length; i++) {\r\n\t\t\t\tvar rEnt = rEnts[i];\r\n\t\t\t\tif (rEnt.name === 'Skybox_sphere' || rEnt.name === 'Skybox_box') {\r\n\t\t\t\t\tvar depthState = rEnt.meshRendererComponent.materials[0].depthState.enabled;\r\n\t\t\t\t\tvar renderQueue = rEnt.meshRendererComponent.materials[0].renderQueue;\r\n\t\t\t\t\tvar cullMode = rEnt.meshRendererComponent.cullMode;\r\n\t\t\t\t\twaterRenderer.setSkyBox(rEnt);\r\n\t\t\t\t\trEnt.meshRendererComponent.materials[0].depthState.enabled = depthState;\r\n\t\t\t\t\trEnt.meshRendererComponent.materials[0].renderQueue = renderQueue;\r\n\t\t\t\t\trEnt.meshRendererComponent.cullMode = cullMode;\r\n\t\t\t\t\tbreak;\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\r\n\t\t\tctx.entityData.waterRenderer = waterRenderer;\r\n\t\t}\r\n\t});\r\n};\r\n\r\nvar cleanup = function(args, ctx, goo) {\r\n\tctx.world.gooRunner.renderSystem.preRenderers.length = 0;\r\n};\r\n\r\nvar update = function(args, ctx, goo) {\r\n};\r\n\r\nvar parameters = [\r\n{\r\n\tkey: 'useRefraction',\r\n\tname: 'useRefraction',\r\n\ttype: 'boolean',\r\n\tdefault: false\r\n},\r\n{\r\n\tkey: 'tex',\r\n\tname: 'Texture',\r\n\ttype: 'texture',\r\n\tdefault: null\r\n},\r\n{\r\n\tkey: 'divider',\r\n\tname: 'Divider',\r\n\ttype: 'int',\r\n\tmin: 1,\r\n\tmax: 5,\r\n\tdefault: 2\r\n},\r\n{\r\n\tkey: 'timeMultiplier',\r\n\tname: 'timeMultiplier',\r\n\ttype: 'float',\r\n\tmin: 0.0,\r\n\tmax: 10,\r\n\tprecision: 3,\r\n\tscale: 0.1,\r\n\tdefault: 1\r\n},\r\n{\r\n\tkey: 'distortionMultiplier',\r\n\tname: 'distortionMultiplier',\r\n\ttype: 'float',\r\n\tmin: 0.01,\r\n\tmax: 1,\r\n\tprecision: 3,\r\n\tscale: 0.1,\r\n\tdefault: 0.04\r\n},\r\n{\r\n\tkey: 'fresnelPow',\r\n\tname: 'fresnelPow',\r\n\ttype: 'float',\r\n\tmin: 0.01,\r\n\tmax: 10,\r\n\tprecision: 3,\r\n\tscale: 0.1,\r\n\tdefault: 1.5\r\n},\r\n{\r\n\tkey: 'normalMultiplier',\r\n\tname: 'normalMultiplier',\r\n\ttype: 'float',\r\n\tmin: 0.01,\r\n\tmax: 1,\r\n\tprecision: 3,\r\n\tscale: 0.1,\r\n\tdefault: 1\r\n},\r\n{\r\n\tkey: 'fresnelMultiplier',\r\n\tname: 'fresnelMultiplier',\r\n\ttype: 'float',\r\n\tmin: 0.01,\r\n\tmax: 1,\r\n\tprecision: 3,\r\n\tscale: 0.1,\r\n\tdefault: 0.5\r\n},\r\n{\r\n\tkey: 'waterScale',\r\n\tname: 'WaterScale',\r\n\ttype: 'float',\r\n\tcontrol: 'slider',\r\n\tmin: 1,\r\n\tmax: 50,\r\n\tprecision: 3,\r\n\tdefault: 20\r\n},\r\n{\r\n\tkey: 'waterColor',\r\n\tname: 'WaterColor',\r\n\ttype: 'vec3',\r\n\tcontrol: 'color',\r\n\tdefault: [0.1, 0.2, 0.3]\r\n},\r\n{\r\n\tkey: 'doFog',\r\n\tname: 'doFog',\r\n\ttype: 'boolean',\r\n\tdefault: false\r\n},\r\n{\r\n\tkey: 'fogColor',\r\n\tname: 'FogColor',\r\n\ttype: 'vec3',\r\n\tcontrol: 'color',\r\n\tdefault: [1, 1, 1]\r\n},\r\n{\r\n\tkey: 'fogStart',\r\n\tname: 'fogStart',\r\n\ttype: 'float',\r\n\tmin: 0.0,\r\n\tmax: 10000,\r\n\tprecision: 3,\r\n\tscale: 0.1,\r\n\tdefault: 500\r\n},\r\n{\r\n\tkey: 'fogScale',\r\n\tname: 'fogScale',\r\n\ttype: 'float',\r\n\tmin: 0.0,\r\n\tmax: 10000,\r\n\tprecision: 3,\r\n\tscale: 0.1,\r\n\tdefault: 2000\r\n},\r\n{\r\n\tkey: 'sunShininess',\r\n\tname: 'sunShininess',\r\n\ttype: 'float',\r\n\tmin: 0.0,\r\n\tmax: 500,\r\n\tprecision: 3,\r\n\tscale: 0.1,\r\n\tdefault: 150\r\n},\r\n{\r\n\tkey: 'sunSpecPower',\r\n\tname: 'sunSpecPower',\r\n\ttype: 'float',\r\n\tmin: 0.0,\r\n\tmax: 10,\r\n\tprecision: 3,\r\n\tscale: 0.1,\r\n\tdefault: 1\r\n},\r\n{\r\n\tkey: 'sunColor',\r\n\tname: 'sunColor',\r\n\ttype: 'vec3',\r\n\tcontrol: 'color',\r\n\tdefault: [1.0, 1.0, 0.5]\r\n},\r\n{\r\n\tkey: 'sunDirection',\r\n\tname: 'sunDirection',\r\n\ttype: 'vec3',\r\n\tdefault: [0, 1, 0.1]\r\n}\r\n];\r\n\r\n/**\r\n * Parameters defined here will be available on the 'args' object as 'args.key'\r\n * and customizable using the script panel. Parameters are defined like below.\r\n * 'key', 'type', and 'default' are required properties.\r\n * {\r\n *   key: string,\r\n *   name: string,\r\n *   type: enum ('int', 'float', 'string', 'boolean', 'vec3'),\r\n *   control: enum (\r\n *    'slider', // Slider with min and max values.\r\n *    'color',  // Color picker for RGB vec3.\r\n *    'select', // Used together with the 'options' property.\r\n *   ),\r\n *   description: string, // Short tooltip description of the parameter.\r\n *   options: *[], // Array of values of specified type.\r\n *   default: *, // Preselected value. One of the options if options are used.\r\n *   min: number, // Can be used when data type is float or int.\r\n *   max: number, // Can be used when data type is float or int.\r\n *   precision: number, // Number of significant digits for float values.\r\n *   scale: number, // How fast number values change when dragging slider.\r\n *   exponential: boolean // Can be used together with slider.\r\n * }\r\n */\r\n \r\n", "name": "Water Script", "license": "CC0", "created": "2015-06-25T08:21:43.572303+00:00", "deleted": false, "dataModelVersion": 2, "modified": "2015-06-25T08:21:45.417556+00:00", "dependencies": {"https://code.gooengine.com/0.14.2/lib/waterpack.js": {"url": "https://code.gooengine.com/0.14.2/lib/waterpack.js", "sortValue": 0}}, "id": "70e02155cb084f17a44b75ad1d78fed8.script"}, "94f4de582abd4e4eb2264956f385c76c.material": {"texturesMapping": {"DIFFUSE_MAP": {"enabled": true, "textureRef": "6be76ed4b4ca46e090ab449d2cc5b506.texture"}, "NORMAL_MAP": {"enabled": true, "textureRef": "fdfefca5704d401cbebc42df3a173bce.texture"}}, "flat": false, "cullState": {"frontFace": "CCW", "enabled": true, "cullFace": "Back"}, "uniforms": {"refractivity": {"enabled": false, "value": 0}, "opacity": {"enabled": false, "value": 1}, "materialAmbient": {"enabled": true, "value": [0.1, 0.1, 0.1, 1]}, "materialSpecular": {"enabled": true, "value": [0.6, 0.6, 0.6, 1]}, "fresnel": {"enabled": true, "value": 0.5}, "reflectivity": {"enabled": true, "value": 0.6}, "materialSpecularPower": {"enabled": true, "value": 64}, "materialDiffuse": {"enabled": true, "value": [0.7466666666666666, 0.2663111111111111, 0.2663111111111111, 1]}, "vertexColorAmount":